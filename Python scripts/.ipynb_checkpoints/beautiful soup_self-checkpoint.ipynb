   "source": [
    "import urllib.request, urllib.parse, urllib.error\n",
    "from bs4 import BeautifulSoup\n",
    "url=input(\"Enter url here : \")\n",
    "html=urllib.request.urlopen(url)\n",
    "for line in html:\n",
    "    print(line)\n",
    "soup=BeautifulSoup(html,'html.parser')\n",
    "tags=soup('a')\n",
    "for tag in tags :\n",
    "    print(tag.get(\"href\",None))"
   ]
  }
